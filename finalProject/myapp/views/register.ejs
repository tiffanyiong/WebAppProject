<title>Register</title>
<%- include partials/head %>
<body id="page-top" class="contact-us">
<!-- Preloader -->
<div class="preloader"></div>
<header>
	<!-- Navigation -->
	<%- include partials/nav %>
	<div id="page_header">
		<div class="page_title">
			<h3>Register</h3>
			
		</div>
	</div>
</header>
<main id="contact">
	 <div class="container">
				<form action="/users/register" class="validated-form" method="POST"  validate>
					<div class="row">
                     <div class="col-lg-12 padding-top-bottom">
						<div class="col-xs-12 col-md-10 col-md-offset-1">
							<div class="form-holder">
								<div class="form-group">
									<input type="text" class="form-control" placeholder="Username *" id="username" name="username" required data-validation-required-message="Please enter your username.">
									<p class="help-block text-danger"></p>
								</div>
								<div class="form-group">
									<input type="email" class="form-control" placeholder="Your Email *" id="email"  name="email" required data-validation-required-message="Please enter your email address.">
									<p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
									<input type="password" class="form-control" placeholder="password *" id="password" name="password" required data-validation-required-message="Please enter your password.">
									<p class="help-block text-danger"></p>
								</div>
								<div class="form-group">
								
								</div>
								<div id="success"></div>
								<button type="submit" class="btn btn-primary">Register</button>
							</div>
                        </div>
                        </div>
					</div>
				</form>
			</div>
		</div>
	</div>
</main>

<%- include partials/footer %>

</body>
</html>